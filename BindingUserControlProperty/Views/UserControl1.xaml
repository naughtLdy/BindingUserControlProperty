<UserControl x:Class="BindingUserControlProperty.Views.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BindingUserControlProperty.Views"
             xmlns:viewModels="clr-namespace:BindingUserControlProperty.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <UserControl.DataContext>
        <viewModels:UserControl1ViewModel/>
    </UserControl.DataContext>
    
	<StackPanel>
        <TextBox Text="{Binding InputParameter.InputText1}" />
        <TextBox Text="{Binding InputParameter.InputText2}" />
    </StackPanel>
</UserControl>
